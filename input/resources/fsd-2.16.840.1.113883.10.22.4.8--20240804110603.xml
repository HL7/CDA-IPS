    <name value="IPSProblemEntry"/>
    <title value="IPS Problem Entry"/>
    <status value="draft"/>
    <experimental value="false"/>
    <date value="2024-08-04T11:06:03Z"/>
    <publisher value="HL7 International"/>
    <contact>
        <name value=""/>
