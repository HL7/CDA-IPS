            <representation value="xmlAttr"/>
            <min value="1"/>
            <max value="1"/>
            <type>
                <code value="string"/>
            </type>
            <fixedString value="IVL_TS"/>
        </element>
        <element id="Supply.effectiveTime.low">
            <path value="Supply.effectiveTime.low"/>
            <definition value="The lower bound of the interval represents the start date/time."/>
            <min value="1"/>
            <max value="1"/>
            <mustSupport value="true"/>
        </element>
        <element id="Supply.effectiveTime.high">
            <path value="Supply.effectiveTime.high"/>
            <definition value="The upper bound represents the end date/time. If it is not present, the device is still used by or present in the patient."/>
            <min value="0"/>
            <max value="1"/>
        </element>
