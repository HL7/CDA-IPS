            <max value="1"/>
            <type>
                <code value="string"/>
            </type>
            <fixedString value="2.16.840.1.113883.10.22.4.26"/>
        </element>
        <element id="Supply.id">
            <path value="Supply.id"/>
            <definition value="This optional element identifies the provision of the device (e.g. implant procedure)"/>
            <min value="0"/>
            <max value="*"/>
            <mustSupport value="true"/>
        </element>
        <element id="Supply.text">
            <path value="Supply.text"/>
            <min value="0"/>
            <max value="1"/>
