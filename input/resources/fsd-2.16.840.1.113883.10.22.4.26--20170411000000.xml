            <representation value="typeAttr"/>
            <definition value="This element provides the interval of time corresponding to the device usage by/presence in the patient."/>
            <min value="1"/>
            <max value="1"/>
            <mustSupport value="true"/>
        </element>
        <element id="Supply.effectiveTime.type">
            <extension url="http://hl7.org/fhir/StructureDefinition/elementdefinition-namespace">
                <valueUri value="http://www.w3.org/2001/XMLSchema-instance"/>
